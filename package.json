{
  "name": "real-config",
  "version": "1.2.0",
  "private": false,
  "description": "Config for commitlint, eslint, jest, npm-package-json-lint, php-cs-fixer, phpstan, stylelint, typedoc, and typescript",
  "keywords": [
    "commitlint",
    "config",
    "eslint",
    "jest",
    "php-cs-fixer",
    "stylelint",
    "typedoc",
    "typescript"
  ],
  "homepage": "https://github.com/ilyub/real-config",
  "bugs": {
    "url": "https://github.com/ilyub/real-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/real-config.git"
  },
  "license": "ISC",
  "author": "Ilia Liubinskii <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "api/index.js",
  "files": [
    "/index.d.ts",
    "/api",
    "/bin",
    "/commitlint",
    "/eslint",
    "/jest",
    "/package-json-lint",
    "/php-cs-fixer",
    "/phpstan",
    "/stylelint",
    "/tsconfig",
    "/typedoc"
  ],
  "scripts": {
    "add-tags": "php bin/add-tags.php",
    "browserslist": "browserslist",
    "clear-cache": "cf cache node_modules/.cache",
    "commitlint": "commitlint --from=",
    "commitlint-next": "commitlint --config commitlint-next.config.js --edit",
    "composer:bump": "composer bump",
    "composer:dump-autoload": "composer dump-autoload",
    "composer:outdated": "composer outdated",
    "composer:reinstall": "composer reinstall *",
    "composer:update": "composer update",
    "config-lint": "node bin/config-lint.js",
    "delete-tags": "php bin/delete-tags.php",
    "full-check": "php bin/full-check.php",
    "git:unix": "git config --replace-all core.ignorecase false",
    "git:windows": "git config --replace-all core.ignorecase true",
    "husky:commit-msg": "npm run commitlint-next",
    "husky:post-commit": "php bin/post-commit.php",
    "husky:pre-commit": "php bin/pre-commit.php",
    "husky:unset-hooks": "git config --unset core.hooksPath",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern **/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern **/*.{js,ts,vue}",
    "lint-no-fix-profile": "set TIMING=1 && eslint --max-warnings 0 --no-error-on-unmatched-pattern **/*.{js,ts,vue}",
    "npm:install": "npm install --no-audit --strict-peer-deps",
    "npm:install-clean": "npm install-clean --no-audit --strict-peer-deps",
    "npm:regenerate-lock-file": "npm install --no-audit --package-lock-only --strict-peer-deps",
    "npm:update": "npm update --no-audit --strict-peer-deps",
    "package-json-lint": "npmPkgJsonLint ./package.json",
    "php-cs-fixer": "php vendor/bin/php-cs-fixer fix --verbose",
    "phpstan": "php vendor/bin/phpstan analyse -c phpstan.neon",
    "phpstan-quiet": "php vendor/bin/phpstan analyse -c phpstan.neon --quiet",
    "prepare": "node bin/prepare.js",
    "sonar": "sonar-scanner",
    "stylelint": "stylelint --fix --max-warnings 0 src/**/*.{css,scss}",
    "stylelint-html": "stylelint --config .stylelintrc-html.js --fix --max-warnings 0 src/**/*.{html,php,vue}",
    "stylelint-html-no-fix": "stylelint --config .stylelintrc-html.js --max-warnings 0 src/**/*.{html,php,vue}",
    "stylelint-no-fix": "stylelint --max-warnings 0 src/**/*.{css,scss}",
    "test": "cf ./lcov-report && jest",
    "test-fast": "cf ./lcov-report && jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,peer,prod",
    "update-package-force": "ncu --dep dev,peer,prod --upgrade",
    "upgrade": "npm run update-package-force && npm run npm:install"
  },
  "dependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jest/test-sequencer": "^29.0.3",
    "@types/eslint": "^8.4.6",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vue/vue3-jest": "^29.1.0",
    "clear-folder": "^3.2.1",
    "cpy-cli": "^4.2.0",
    "css-property-sort-order-smacss": "^2.2.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-escompat": "^3.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-misc": "^1.2.2",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-regexp": "^1.9.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-imports-requires": "^1.0.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "eslint-plugin-xss": "^0.1.12",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-environment-node": "^29.0.3",
    "jest-transform-stub": "^2.0.0",
    "jsonschema": "^1.4.1",
    "npm-check-updates": "^16.2.1",
    "npm-package-json-lint": "^6.3.0",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "publish-if-not-exists": "^1.1.0",
    "strip-ansi-cli": "^3.0.2",
    "stylelint": "^14.12.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.4",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^29.0.1",
    "ts-transform-import-path-rewrite": "^0.3.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.15",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^0.40.13"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
