{
  "properties": {
    "extends": {
      "enum": [
        "./tsconfig/min",
        "./tsconfig/strict",
        "real-config/tsconfig/min",
        "real-config/tsconfig/strict"
      ]
    },
    "compilerOptions": {
      "properties": {
        "baseUrl": {
          "const": "."
        },
        "outDir": {
          "const": "cache/typescript"
        },
        "paths": {
          "properties": {
            "@": {
              "items": {
                "const": "src"
              }
            },
            "@/*": {
              "items": {
                "const": "src/*"
              }
            }
          },
          "required": [
            "@",
            "@/*"
          ]
        },
        "typeRoots": {
          "items": {
            "enum": [
              "node_modules/@types",
              "node_modules/types-fix"
            ]
          }
        }
      },
      "required": [
        "baseUrl",
        "outDir",
        "paths"
      ]
    },
    "include": {
      "items": {
        "enum": [
          "*.js",
          "*.ts",
          ".eslintrc.js",
          ".eslintrc.overrides.js",
          ".eslintrc.rule-overrides.js",
          ".eslintrc.synonyms.js",
          ".eslintrc.temp.js",
          ".postcssrc.js",
          ".prettierrc.js",
          ".stylelintrc-html.js",
          ".stylelintrc.js",
          "__mocks__/**/*.js",
          "__mocks__/**/*.ts",
          "__mocks__/**/*.tsx",
          "__mocks__/**/*.vue",
          "api/**/*.js",
          "api/**/*.ts",
          "api/**/*.tsx",
          "api/**/*.vue",
          "bin/**/*.js",
          "bin/**/*.ts",
          "bin/**/*.tsx",
          "bin/**/*.vue",
          "commitlint/**/*.js",
          "commitlint/**/*.ts",
          "commitlint/**/*.tsx",
          "commitlint/**/*.vue",
          "eslint/**/*.js",
          "eslint/**/*.ts",
          "eslint/**/*.tsx",
          "eslint/**/*.vue",
          "fixtures/**/*.js",
          "fixtures/**/*.ts",
          "fixtures/**/*.tsx",
          "fixtures/**/*.vue",
          "jest/**/*.js",
          "jest/**/*.ts",
          "jest/**/*.tsx",
          "jest/**/*.vue",
          "sample/**/*.js",
          "sample/**/*.ts",
          "sample/**/*.tsx",
          "sample/**/*.vue",
          "src/**/*.js",
          "src/**/*.ts",
          "src/**/*.tsx",
          "src/**/*.vue",
          "stylelint/**/*.js",
          "stylelint/**/*.ts",
          "stylelint/**/*.tsx",
          "stylelint/**/*.vue",
          "tests/**/*.js",
          "tests/**/*.ts",
          "tests/**/*.tsx",
          "tests/**/*.vue"
        ]
      }
    }
  },
  "required": [
    "compilerOptions",
    "exclude",
    "extends",
    "include"
  ]
}
