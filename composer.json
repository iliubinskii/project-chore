{
  "name": "real/config",
  "description": "Config for commitlint, eslint, jest, npm-package-json-lint, php-cs-fixer, phpstan, stylelint, typedoc, and typescript.",
  "type": "library",
  "license": "ISC",
  "homepage": "https://github.com/ilyub/real-config",
  "archive": {
    "exclude": [
      "!/composer.json",
      "!/composer.lock",
      "!/package.json",
      "!/{api,bin,src}/**/*.{php,neon}",
      "*",
      ".*"
    ]
  },
  "authors": [
    {
      "name": "Ilia Liubinskii",
      "email": "admin.phpdev@gmail.com"
    }
  ],
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.12",
    "phpstan/phpstan": "^1.8.10"
  }
}
