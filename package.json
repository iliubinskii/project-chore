{
  "name": "real-config",
  "version": "1.18.0",
  "private": false,
  "description": "Config for commitlint, eslint, jest, npm-package-json-lint, php-cs-fixer, phpstan, stylelint, typedoc, and typescript.",
  "keywords": [
    "commitlint",
    "config",
    "eslint",
    "jest",
    "php-cs-fixer",
    "phpstan",
    "stylelint",
    "typedoc",
    "typescript"
  ],
  "homepage": "https://github.com/ilyub/real-config",
  "bugs": {
    "url": "https://github.com/ilyub/real-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/real-config.git"
  },
  "license": "ISC",
  "author": "Ilia Liubinskii <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "api/index.js",
  "files": [
    "/index.d.ts",
    "/api",
    "/bin",
    "/commitlint",
    "/eslint",
    "/jest",
    "/package-json-lint",
    "/php-cs-fixer",
    "/phpstan",
    "/stylelint",
    "/tsconfig",
    "/typedoc"
  ],
  "scripts": {
    "add-tags": "php bin/add-tags.php",
    "browserslist": "browserslist",
    "build-changelog": "auto-changelog",
    "clear-cache": "cf cache node_modules/.cache",
    "commitlint": "commitlint --from=",
    "commitlint-next": "commitlint --config=commitlint-next.config.js --edit",
    "composer:bump": "composer bump",
    "composer:dump-autoload": "composer dump-autoload",
    "composer:outdated": "composer outdated",
    "composer:reinstall": "composer reinstall *",
    "composer:update": "composer update",
    "config-lint": "node bin/config-lint.js",
    "delete-tags": "php bin/delete-tags.php",
    "full-check": "php bin/full-check.php",
    "git:unix": "git config --replace-all core.ignorecase false",
    "git:windows": "git config --replace-all core.ignorecase true",
    "husky:commit-msg": "npm run commitlint-next",
    "husky:pre-commit": "php bin/pre-commit.php",
    "husky:unset-hooks": "git config --unset core.hooksPath",
    "lint": "eslint --fix --max-warnings=0 --no-error-on-unmatched-pattern **/*.{js,ts,tsx,vue}",
    "lint-no-fix": "eslint --max-warnings=0 --no-error-on-unmatched-pattern **/*.{js,ts,tsx,vue}",
    "lint-no-fix-profile": "set TIMING=1 && eslint --max-warnings=0 --no-error-on-unmatched-pattern **/*.{js,ts,tsx,vue}",
    "major-release": "php bin/major-release.php",
    "markdownlint": "markdownlint *.md --ignore=CHANGELOG.md",
    "ncu": "ncu --dep=dev,peer,prod",
    "ncu-upgrade": "ncu --dep=dev,peer,prod --upgrade",
    "next-release": "php bin/next-release.php",
    "npm:install": "npm install --no-audit --strict-peer-deps",
    "npm:install-clean": "npm install-clean --no-audit --strict-peer-deps",
    "npm:regenerate-lock-file": "npm install --no-audit --package-lock-only --strict-peer-deps",
    "npm:update": "npm update --no-audit --strict-peer-deps",
    "package-json-lint": "npmPkgJsonLint ./package.json",
    "php-cs-fixer": "php vendor/bin/php-cs-fixer fix --verbose",
    "phpstan": "php vendor/bin/phpstan analyse -c phpstan.neon",
    "phpstan-quiet": "php vendor/bin/phpstan analyse -c phpstan.neon --quiet",
    "prepare": "node bin/prepare.js",
    "stylelint": "stylelint --allow-empty-input --fix --max-warnings=0 src/**/*.{css,scss}",
    "stylelint-html": "stylelint --allow-empty-input --config=.stylelintrc-html.js --fix --max-warnings=0 src/**/*.{html,php,vue}",
    "stylelint-html-no-fix": "stylelint --allow-empty-input --config=.stylelintrc-html.js --max-warnings=0 src/**/*.{html,php,vue}",
    "stylelint-no-fix": "stylelint --allow-empty-input --max-warnings=0 src/**/*.{css,scss}",
    "test": "cf coverage/lcov-report && jest",
    "test-fast": "cf coverage/lcov-report && jest --max-workers=4",
    "test-release": "jest --coverage-directory=. --coverage-reporters=lcovonly",
    "tsc": "tsc --noEmit",
    "upgrade": "npm run ncu-upgrade && npm run npm:install"
  },
  "dependencies": {
    "jsonschema": "^1.4.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "@commitlint/cli": ">=17",
    "@commitlint/config-conventional": ">=17",
    "@jest/expect-utils": ">=29",
    "@jest/test-sequencer": ">=29",
    "@types/eslint": ">=8",
    "@types/jest": ">=29",
    "@types/node": ">=18",
    "@typescript-eslint/eslint-plugin": ">=5",
    "@typescript-eslint/parser": ">=5",
    "@vue/vue3-jest": ">=29",
    "auto-changelog": ">=2",
    "clear-folder": ">=5",
    "cpy-cli": ">=4",
    "css-property-sort-order-smacss": ">=2",
    "del-cli": ">=5",
    "eslint": ">=8",
    "eslint-config-prettier": ">=8",
    "eslint-import-resolver-typescript": ">=3",
    "eslint-plugin-deprecation": ">=1",
    "eslint-plugin-es": ">=4",
    "eslint-plugin-escompat": ">=3",
    "eslint-plugin-eslint-comments": ">=3",
    "eslint-plugin-etc": ">=2",
    "eslint-plugin-github": ">=4",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-jest": ">=27",
    "eslint-plugin-jest-extended": ">=2",
    "eslint-plugin-jsdoc": ">=39",
    "eslint-plugin-misc": "^1.8.2",
    "eslint-plugin-no-type-assertion": ">=1",
    "eslint-plugin-no-use-extend-native": ">=0",
    "eslint-plugin-node": ">=11",
    "eslint-plugin-only-warn": ">=1",
    "eslint-plugin-pii": ">=1",
    "eslint-plugin-prettier": ">=4",
    "eslint-plugin-promise": ">=6",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=4",
    "eslint-plugin-react-native": ">=4",
    "eslint-plugin-regexp": ">=1",
    "eslint-plugin-security": ">=1",
    "eslint-plugin-sonarjs": ">=0",
    "eslint-plugin-sort-destructure-keys": ">=1",
    "eslint-plugin-sort-imports-requires": ">=1",
    "eslint-plugin-spellcheck": ">=0",
    "eslint-plugin-typescript-sort-keys": ">=2",
    "eslint-plugin-unicorn": ">=45",
    "eslint-plugin-unused-imports": ">=2",
    "eslint-plugin-vue": ">=9",
    "eslint-plugin-vue-scoped-css": ">=2",
    "eslint-plugin-xss": ">=0",
    "husky": ">=8",
    "jest": ">=29",
    "jest-environment-jsdom": ">=29",
    "jest-environment-node": ">=29",
    "jest-matcher-utils": ">=29",
    "jest-transform-stub": ">=2",
    "markdownlint-cli": ">=0",
    "npm-check-updates": ">=16",
    "npm-package-json-lint": ">=6",
    "postcss-html": ">=1",
    "postcss-scss": ">=4",
    "prettier": ">=2",
    "strip-ansi-cli": ">=3",
    "stylelint": ">=14",
    "stylelint-config-standard-scss": ">=6",
    "stylelint-no-unsupported-browser-features": ">=6",
    "stylelint-order": ">=6",
    "ts-jest": ">=29",
    "typedoc": ">=0",
    "typedoc-plugin-markdown": ">=3",
    "typescript": ">=4",
    "vue-eslint-parser": ">=9",
    "vue-tsc": ">=1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
